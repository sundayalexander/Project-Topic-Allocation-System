<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block meta %}
            <meta name="viewport" content="width=device-width, initial-scale=1">
            <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
        {% endblock %}
        {% block favicon %}
            <!-- Favicon -->
            <link rel="apple-touch-icon" sizes="57x57" href="{{ asset('img/favicon/apple-icon-57x57.png') }}">
            <link rel="apple-touch-icon" sizes="60x60" href="{{ asset('img/favicon/apple-icon-60x60.png') }}">
            <link rel="apple-touch-icon" sizes="72x72" href="{{ asset('img/favicon/apple-icon-72x72.png') }}">
            <link rel="apple-touch-icon" sizes="76x76" href="{{ asset('img/favicon/apple-icon-76x76.png') }}">
            <link rel="apple-touch-icon" sizes="114x114" href="{{ asset('img/favicon/apple-icon-114x114.png') }}">
            <link rel="apple-touch-icon" sizes="120x120" href="{{ asset('img/favicon/apple-icon-120x120.png') }}">
            <link rel="apple-touch-icon" sizes="144x144" href="{{ asset('img/favicon/apple-icon-144x144.png') }}">
            <link rel="apple-touch-icon" sizes="152x152" href="{{ asset('img/favicon/apple-icon-152x152.png') }}">
            <link rel="apple-touch-icon" sizes="180x180" href="{{ asset('img/favicon/apple-icon-180x180.png') }}">
            <link rel="icon" type="image/png" sizes="192x192"  href="{{ asset('img/favicon/android-icon-192x192.png') }}">
            <link rel="icon" type="image/png" sizes="32x32" href="{{ asset('img/favicon/favicon-32x32.png') }}">
            <link rel="icon" type="image/png" sizes="96x96" href="{{ asset('img/favicon/favicon-96x96.png') }}">
            <link rel="icon" type="image/png" sizes="16x16" href="{{ asset('img/favicon/favicon-16x16.png') }}">
            <link rel="shortcut icon" href="{{ asset('img/favicon/favicon.ico') }}" type="image/x-icon" />
            <link rel="apple-touch-icon" href="{{ asset('img/favicon/apple-icon.png') }}">
            <link rel="manifest" href="{{ asset('img/favicon/manifest.json') }}">
        {% endblock %}
        {% block stylesheets %}
            <script type="application/x-javascript"> addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false); function hideURLbar(){ window.scrollTo(0,1); } </script>
            <!-- Bootstrap Core CSS -->
            <link href="{{ asset('control_panel/css/bootstrap.css') }}" rel='stylesheet' type='text/css' />
            <!-- Custom CSS -->
            <link href="{{ asset('control_panel/css/style.css') }}" rel='stylesheet' type='text/css' />
            <!-- font CSS -->
            <!-- font-awesome icons -->
            <link href="{{ asset('control_panel/css/font-awesome.css') }}" rel="stylesheet">
            <!-- //font-awesome icons -->
            <!-- js-->
            <script src="{{ asset('control_panel/js/jquery-1.11.1.min.js') }}"></script>
            <script src="{{ asset('control_panel/js/modernizr.custom.js') }}"></script>
            <!--webfonts-->
            <link href='https://fonts.googleapis.com/css?family=Roboto+Condensed:400,300,300italic,400italic,700,700italic' rel='stylesheet' type='text/css'>
            <!--//webfonts-->
            <!--animate-->
            <link href="{{ asset('control_panel/css/animate.css') }}" rel="stylesheet" type="text/css" media="all">
            <script src="{{ asset('control_panel/js/wow.min.js') }}"></script>
            <script>
                new WOW().init();
            </script>
            <!--//end-animate-->
            <!-- chart -->
            <script src="{{ asset('control_panel/js/Chart.js') }}"></script>
            <!-- //chart -->
            <!--Calender-->
            <link rel="stylesheet" href="{{ asset('control_panel/css/clndr.css') }}" type="text/css" />
            <script src="{{ asset('control_panel/js/underscore-min.js') }}" type="text/javascript"></script>
            <script src= "{{ asset('control_panel/js/moment-2.2.1.js') }}" type="text/javascript"></script>
            <script src="{{ asset('control_panel/js/clndr.js') }}" type="text/javascript"></script>
            <script src="{{ asset('control_panel/js/site.js') }}" type="text/javascript"></script>
            <!--End Calender-->
            <!-- Metis Menu -->
            <script src="{{ asset('control_panel/js/metisMenu.min.js') }}"></script>
            <script src="{{ asset('control_panel/js/custom.js') }}"></script>
            <link href="{{ asset('control_panel/css/custom.css') }}" rel="stylesheet">
            <!--//Metis Menu -->
        {% endblock %}
    </head>
    <body class="cbp-spmenu-push">
        {% block body %}{% endblock %}
        {% block javascripts %}{% endblock %}
    </body>
</html>
